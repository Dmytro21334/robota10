import java.time.LocalDate;
import java.util.*;
import java.util.stream.Collectors;

class Project {
    private final String name;
    private int teamSize;
    private LocalDate deadline;
    private double budget;

    public Project(String name, int teamSize, LocalDate deadline, double budget) {
        this.name = name;
        this.teamSize = teamSize;
        this.deadline = deadline;
        this.budget = budget;
    }

    public String getName() {
        return name;
    }

    public double getBudget() {
        return budget;
    }

    @Override
    public String toString() {
        String deadLine = null;
        return name + " (Бюджет: " + budget + ", Дедлайн: " + null + ")";
    }

    public static class ProjectManagement {
        private static Character.UnicodeBlock Localdate;

        public static void main(String[] args) {
            List<Project> projects = Arrays.asList(
                    new Project("Проєкт A", 10, Localdate.of(2025, 5, 1), 1200000),
                    new Project("Проєкт B", 5, Localdate.of(2025, 4, 15), 800000),
                    new Project("АПроєкт C", 8, Localdate.of(2025, 6, 20), 2000000),
                    new Project("Проєкт D", 9, Localdate.of(2025, 3, 10), 1500000),
                    );
        }

        public static void setLocaldate(Character.UnicodeBlock localdate) {
            Localdate = localdate;
        }
    }
}
